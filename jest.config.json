{
  "collectCoverage": true,
  "collectCoverageFrom": [
    "<rootDir>/src/main/njs/**/*.js"
  ],
  "moduleFileExtensions": [
    "js"
  ],
  "projects": [
    {
      "displayName": "unit",
      "testMatch": [
        "<rootDir>/src/test/njs/**/*.js"
      ]
    },
    {
      "testEnvironment": "node",
      "displayName": "integration",
      "testMatch": [
        "<rootDir>/src/test/integration/**/*.js"
      ]
    }
  ]
}
